<html xmlns:tal="http://xml.zope.org/namespaces/tal"
        xmlns:metal="http://xml.zope.org/namespaces/metal"
        i18n:domain="plone">

<body>

<div metal:define-macro="portlet">

  <tal:comment replace="nothing">
    Dept Info
  </tal:comment>

  <div id="ownership">

    <a i18n:translate="tabs_home" tal:attributes="href python:portal_url">  
      <span tal:content="portal_title" tal:omit-tag="" />
    </a>

    <p class="address">

      <span tal:content="portal_properties/ucdploneskin_properties/address1" tal:omit-tag="" />
      <tal:br tal:condition="portal_properties/ucdploneskin_properties/address1">
        <br/>
      </tal:br>

      <span tal:content="portal_properties/ucdploneskin_properties/address2" tal:omit-tag="" />
      <tal:br tal:condition="portal_properties/ucdploneskin_properties/address2">
        <br/>
      </tal:br>

      <span tal:content="portal_properties/ucdploneskin_properties/address3" tal:omit-tag="" />
      <tal:br tal:condition="portal_properties/ucdploneskin_properties/address3">
        <br/>
      </tal:br>

      <span tal:content="portal_properties/ucdploneskin_properties/address4" tal:omit-tag="" />
      <tal:br tal:condition="portal_properties/ucdploneskin_properties/address4">
        <br/>
      </tal:br>

    </p>

    <p>
      <span tal:content="portal_properties/ucdploneskin_properties/phone1" tal:omit-tag="" />
      <span tal:condition="portal_properties/ucdploneskin_properties/phone1" tal:content="string: phone" tal:omit-tag="" />
      <tal:br tal:condition="portal_properties/ucdploneskin_properties/phone1">
        <br/>
      </tal:br>

      <span tal:condition="portal_properties/ucdploneskin_properties/phone2"
            tal:content="portal_properties/ucdploneskin_properties/phone2" tal:omit-tag="" />
      <span tal:condition="portal_properties/ucdploneskin_properties/phone2" tal:content="string: phone" tal:omit-tag="" />
      <tal:br tal:condition="portal_properties/ucdploneskin_properties/phone2">
        <br/>
      </tal:br>

      <span tal:content="portal_properties/ucdploneskin_properties/fax1" tal:omit-tag="" />
      <span tal:condition="portal_properties/ucdploneskin_properties/fax1" tal:content="string: fax" tal:omit-tag="" />
      <tal:br tal:condition="portal_properties/ucdploneskin_properties/fax1">
        <br/>
      </tal:br>

      <span tal:condition="portal_properties/ucdploneskin_properties/fax2"
            tal:content="portal_properties/ucdploneskin_properties/fax2" tal:omit-tag="" />
      <span tal:condition="portal_properties/ucdploneskin_properties/fax2" tal:content="string: fax" tal:omit-tag="" />
      <tal:br tal:condition="portal_properties/ucdploneskin_properties/fax2">
        <br/>
      </tal:br>

    </p>
  
  </div>


</div>

</body>

</html>
