<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="ucdskin">

 <object name="Products_ucdavis_custom_images"
    meta_type="Filesystem Directory View"
    directory="Products.ucdavis:skins/Products_ucdavis_custom_images"/>
 <object name="Products_ucdavis_custom_templates"
    meta_type="Filesystem Directory View"
    directory="Products.ucdavis:skins/Products_ucdavis_custom_templates"/>
 <object name="Products_ucdavis_styles"
    meta_type="Filesystem Directory View"
    directory="Products.ucdavis:skins/Products_ucdavis_styles"/>

 <skin-path name="ucdskin" based-on="Plone Classic Theme">
  <layer name="Products_ucdavis_custom_images"
     insert-after="custom"/>
  <layer name="Products_ucdavis_custom_templates"
     insert-after="Products_ucdavis_custom_images"/>
  <layer name="Products_ucdavis_styles"
     insert-after="Products_ucdavis_custom_templates"/>
 </skin-path>

</object>
